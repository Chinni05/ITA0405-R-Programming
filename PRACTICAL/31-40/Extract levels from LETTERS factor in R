# Generate a random sample from the LETTERS dataset
set.seed(42)  # Setting seed for reproducibility
random_sample <- sample(LETTERS, 10, replace = TRUE)

# Create a factor from the random sample
factor_sample <- factor(random_sample)

# Display the factor
print("Factor levels:")
print(levels(factor_sample))

# Extract the first five levels of the factor
five_levels <- levels(factor_sample)[1:5]

# Display the extracted five levels
print("First five levels:")
print(five_levels)
